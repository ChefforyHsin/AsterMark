<AnchorPane prefHeight="340.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.EmbeddingResultController">
   <children>
      <Label layoutX="170.0" layoutY="28.0" text="嵌入完毕！">
         <font>
