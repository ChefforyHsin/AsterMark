      <RadioButton fx:id="rbtn_currentDataBase" layoutX="193.0" layoutY="223.0" mnemonicParsing="false" text="当前数据库" />
      <RadioButton fx:id="rbtn_csvFile" layoutX="193.0" layoutY="257.0" mnemonicParsing="false" text="csv文件" />
      <Button layoutX="354.0" layoutY="280.0" mnemonicParsing="false" onAction="#beginPublish" text="开始发布">
